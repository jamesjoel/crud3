<div class="container" style="min-height: 600px; height: auto;">
	<h1>All Student</h1>
	<table class="table table-hover table-striped table-bordered">
		<tr>
			<th>S.No.</th>
			<th>First Name</th>
			<th>Last Name</th>
		</tr>
		<% data.forEach(function(x, i){ %>
		<Tr>
			<td><%= i+1 %></td>
			<td><%= x.first_name %></td>
			<td><%= x.last_name %></td>
		</Tr>
	<% }) %>
	</table>
</div>
